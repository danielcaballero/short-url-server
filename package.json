{
  "name": "short-url-server",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "jest"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@model": "dist/model",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server"
  },
  "dependencies": {
    "@types/mongoose": "^5.7.30",
    "@types/nanoid": "^2.1.0",
    "@types/valid-url": "^1.0.3",
    "command-line-args": "^5.1.1",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.22",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.10",
    "valid-url": "^1.0.9",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.11",
    "@types/jest": "^26.0.4",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.20",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "jest": "^26.1.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  }
}
